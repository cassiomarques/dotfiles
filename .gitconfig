[core]
  editor = nvim
	excludesfile = /Users/cassio.marques/.gitignore_global

[user]
	name = CaÃÅssio Marques
	email = cassio.marques@multiverse.io

[alias]
  ci = commit
  cam = commit -am
  co = checkout
  praise = blame

[apply]
  whitespace = strip

[diff]
  color = auto
  rename = copy

[pager]
  color = true

[status]
  color = auto
[push]
  default = matching

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[http]
	proxy =

[commit]
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[pull]
	ff = only
[init]
	defaultBranch = main
